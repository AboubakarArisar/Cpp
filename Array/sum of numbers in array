#include<iostream>
using namespace std;
int main()
{
	int value,size,sum=0;
cout<<"Enter the size of an array you wonder --> ";
cin>>size;
int array_1[size];
for(int i=0; i<size; i++)
{
	cout<<"Enter the number --> ";
	cin>>array_1[i];
	value=array_1[i];
	sum+=value;
}
cout<<"\n\nThe sum of the all values in the array is --> "<<sum<<endl;
return 0;
}
