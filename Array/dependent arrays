
#include<iostream>
using namespace std;
int main()
{
int N[5]={22,45,21,12,3};
int M[6]={23,1,12,13,4,45};
int N_M[5+6];
for(int i=0; i<11; i++)
{
	if(i=0 && i<5)
	{
		N_M[i]=N[i];
	}
	else
	{
		N_M[i]=M[i];
	}
}
cout<<"\nValues in the first array are below : "<<endl;
for(int a=0; a<5; a++)
{
	cout<<N[a]<<" ";
}

cout<<"\nValues in the Second array are below : "<<endl;
for(int b=0; b<6; b++)
{
	cout<<M[b]<<" ";
}

cout<<"\nValues in the third array are below : "<<endl;
for(int c=0; c<11; c++)
{
	cout<<N_M[c]<<" ";
}
return 0;
}















