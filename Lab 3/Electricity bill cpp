#include <iostream>  
#include <string>  
using namespace std;  
int main()  
{  
 string name;  
 int y,z;  
 cout<<"Customer Name?"<<endl;  
 cin>>name;  
 cout<<"Name:"<<name<<endl;  
 cout<<"Enter Customer ID?"<<endl;  
 cin>>y;  
 cout<<"Enter Units Consumed?"<<endl;  
 cin>>z;  
 if (z>=1&&z<=199)  
 {  
 cout<<"You have to pay"<<" "<<5*z<<" "<<"RS"<<endl;   }  
 else if (z>=200&&z<=399)  
 {  
 cout<<"You have to pay"<<" "<<10*z<<" "<<"RS"<<endl;   }  
  
 else if (z>=400&&z<=599)  
 { 
 cout<<"You have to pay"<<" "<<15*z<<" "<<"RS"<<endl;   }  
  
 else if (z>=600)  
 {  
 cout<<"You have to pay"<<" "<<20*z<<" "<<"RS"<<endl;   }  
  
  
  
system ("pause");  
return 0;  
} 
