#include<iostream>
using namespace std;


	struct student {
	int id;
	string name;
	double percent;
	};
	
	
int main()
{  	student s[3];
	for (int i=0; i<3; i++) {
		cout << "Enter ID Of Student " << i+1 << ": ";
		cin >> s[i].id ;
		cout << "Enter Name Of Student " << i+1 << ": ";
		cin >> s[i].name;
		cout << "Enter Percentage Of Student " << i+1 << ": ";
		cin >> s[i].percent;
		cout << endl;
	}
	for (int j=0; j<3; j++) {
	cout << s[j].id << "\t" << s[j].name << "\t" << s[j].percent << endl;
}
return 0;
}
